<%= simple_form_for @project, html:{class:"form-inline"} do |f| %>
	<%= render "form", f:f %>
		<div class="row col-md-12">
			<div class="col-md-6">
				<%= f.button :submit, "Create Project", class:"btn btn-primary col-md-12" %>
			</div>
			<div class="col-md-6">
				<%= link_to "Cancel", :back, class:"btn btn-default col-md-12" %>
			</div>
		</div>
	</div>
<% end %>